add (s64 a, s64 b s64) {
    return a + b;
}

sub (s64 a, s64 b s64) s64 {
    return a - b;
}
mul (s64 a, s64 b s64) s64 {
    return a * b;
}

idiv (s64 a, s64 b s64) s64 {
    return a / b;
}

